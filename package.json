{
  "name": "dutch-lady-onboarding-scheduling",
  "version": "1.0.1",
  "description": "Dutch Lady onboarding & bulk scheduling",
  "main": "dist/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://git@bitbucket.org/kidsloop-vietnam/dl-onboarding-scheduling.git"
  },
  "homepage": "https://bitbucket.org/kidsloop-vietnam/dl-onboarding-scheduling",
  "scripts": {
    "test": "npm run test:unit && npm run test:acceptance",
    "test:unit": "mocha --require ts-node/register \"tests/unit/**/*.test.ts\" --timeout 8000 --exit",
    "test:acceptance": "mocha --require ts-node/register \"tests/acceptance/**/*.test.ts\" --timeout 8000 --exit",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "fmt": "npm run fix:lint && npm run fix:prettier",
    "commitlint-check": "commitlint",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "onboarding",
    "bulk scheduling"
  ],
  "author": "KLVN",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@commitlint/cz-commitlint": "16.2.1",
    "@types/apollo-upload-client": "17.0.0",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/chai-spies": "1.0.3",
    "@types/connect-timeout": "0.0.36",
    "@types/cron": "1.7.3",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.2",
    "@types/mocha": "9.1.0",
    "@types/multer": "1.4.7",
    "@types/response-time": "2.3.5",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-spies": "1.0.0",
    "commitizen": "4.2.4",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-functional": "4.2.0",
    "eslint-plugin-import": "2.25.4",
    "http-errors": "2.0.0",
    "husky": "7.0.4",
    "mocha": "9.2.1",
    "msw": "0.38.1",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "supertest": "6.2.2",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@apollo/client": "3.5.9",
    "apollo-upload-client": "17.0.0",
    "axios": "0.26.0",
    "connect-timeout": "1.9.0",
    "cron": "1.8.2",
    "cross-fetch": "3.1.5",
    "csv-parse": "5.0.4",
    "csv-stringify": "6.0.5",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "graphql": "16.3.0",
    "joi": "17.6.0",
    "multer": "1.4.4",
    "response-time": "2.3.2"
  },
  "prettier": {
    "singleQuote": true
  }
}
